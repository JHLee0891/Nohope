{
  // 화살표 함수 괄호 설정 (하나의 매개변수만 있을 경우 괄호 생략)
  "arrowParens": "avoid",
  // 객체 선언 시 괄호 양 끝 사이 간격 사용 여부
  "bracketSpacing": true,
  // HTML 파일에서 CSS와 같이 공백이 무시되는 경우에 공백을 무시
  "htmlWhitespaceSensitivity": "css",
  // 출력되는 코드의 최대 길이를 지정 (100자)
  "printWidth": 100,
  // 텍스트 형식의 파일에서 문장을 자동으로 줄 바꿈할 때 사용 (명시적으로 줄 바꿈을 추가한 곳에만 줄 바꿈이 유지)
  "proseWrap": "preserve",
  //"as-needed": 이 옵션이 선택된 경우, Prettier는 속성 이름이 특수 문자나 공백을 포함하고 있는 경우에만 따옴표를 추가
  "quoteProps": "as-needed",
  // 문자열을 작은 따옴표('')로 표시
  "singleQuote": true,
  // 코드의 끝에 세미콜론을 추가
  "semi": true,
  // 탭 문자의 너비를 지정
  "tabWidth": 2,
  //객체 또는 배열의 마지막 요소 뒤에 쉼표추가 (현재 마지막 배열에는 , 추가하지 않음으로 되어있음)
  "trailingComma": "none",
  //들여쓰기에 탭 여부
  "useTabs": false
}
